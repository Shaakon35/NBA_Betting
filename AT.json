:root {
	 --white: #fff;
	 --gray: #a0a7c2;
	 --gray-dark: #6c778c;
	 --icon: #a0a7c2;
	 --text-muted: #a0a7c2;
	 --primary-100: var(--lighten_10_primary);
	 --primary-200: var(--lighten_20_primary);
	 --primary-300: var(--lighten_30_primary);
	 --primary-400: var(--lighten_40_primary);
	 --primary-500: var(--lighten_50_primary);
	 --gray-100: #f7f8fa;
	 --gray-200: #eff1f7;
	 --gray-300: #e5e9f7;
	 --gray-400: #e5e9f7;
	 --gray-500: #a0a7c2;
	 --gray-600: #a0a7c2;
	 --gray-700: #6b757d;
	 --gray-800: #6c778c;
	 --gray-900: #0f1e57;
	 --hs-1: #fc0;
	 --hs-2: #6798ff;
	 --hs-3: #1acb9a;
	 --hs-4: #f45a6d;
	 --primary: #4f74ff;
	 --secondary: #6c778c;
	 --success: #35d068;
	 --info: #8777d9;
	 --warning: #ffc400;
	 --danger: #f94627;
	 --light: #f7f8fa;
	 --dark: #6c778c;
	 --main: #0f1e57;
	 --breakpoint-xs: 0;
	 --breakpoint-sm: 576px;
	 --breakpoint-md: 768px;
	 --breakpoint-lg: 992px;
	 --breakpoint-xl: 1200px;
	 --font-family-sans-serif: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, "Noto Sans", sans-serif, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol", "Noto Color Emoji";
	 --font-family-monospace: SFMono-Regular, Menlo, Monaco, Consolas, "Liberation Mono", "Courier New", monospace;
	 --error-width: 750px;
}
 @font-face {
	 src: url(https://volterra.io/fonts/AvenirLTStd-Book.otf);
	 font-family: Avenir;
	 font-weight: 400;
}
 @font-face {
	 src: url(https://volterra.io//fonts/AvenirLTStd-Medium.otf);
	 font-family: Avenir;
	 font-weight: 700;
}
 body {
	 background-color: var(--gray-100);
	 color: var(--main);
	 font-family: Avenir, Helvetica Neue, Helvetica, Arial, sans-serif;
}
 .error-header {
	 padding: 20px 0;
}
 .error-header .container {
	 max-width: 1140px;
	 margin: 0 auto;
}
 .error-body {
	 max-width: var(--error-width);
	 margin: 0 auto;
}
 .error-body h1 {
	 font-size: 3.25rem;
	 font-weight: 400;
	 margin-bottom: 10px;
	 line-height: 1.1em;
}
 .error-body h2 {
	 font-size: 2.25rem;
	 font-weight: 400;
	 line-height: 1.1em;
	 margin-bottom: 14px;
}
 .error-body h5 {
	 font-size: 1rem;
	 margin: 0;
	 line-height: 1.3em;
}
 @media (max-width: 767px) {
	 .error-body h1 {
		 font-size: 1.6rem;
		 margin-bottom: 4px;
	}
	 .error-body h2 {
		 font-size: 1.3rem;
	}
}
 .error-desc h5 {
	 margin-bottom: 6px;
}
 .error-desc p {
	 margin: 0;
	 margin-bottom: 20px;
}
 .error-type {
	 font-size: 2.25rem;
	 color: var(--gray-500);
	 line-height: 1.2em;
	 margin: 0;
}
 @media (max-width: 767px) {
	 .error-type {
		 font-size: 1.3rem;
	}
}
 .flow {
	 display: flex;
	 justify-content: space-between;
	 grid-template-columns: minmax(0px, 1fr) 80px minmax(0px, 1fr) 80px minmax(0px, 1fr);
	 margin: 60px 0;
}
 .flow > * {
	 text-align: center;
}
 @media (max-width: 767px) {
	 .flow {
		 margin: 40px 0;
	}
}
 .flow-link {
	 padding: 46px 10px 0 10px;
}
 @media (max-width: 767px) {
	 .flow-link {
		 padding-top: 18px;
	}
	 .flow-link svg {
		 width: 24px;
		 height: auto;
	}
}
 .flow-step p {
	 margin: 16px 0;
	 line-height: 1.2em;
}
 .flow-step .flow-title {
	 font-size: 2.25rem;
	 color: var(--gray-500);
	 margin: 0;
	 line-height: 1.1em;
}
 @media (max-width: 767px) {
	 .flow-step p {
		 font-size: 0.8rem;
		 margin: 10px 0;
	}
	 .flow-step .flow-title {
		 font-size: 1.2rem;
	}
}
 .flow-icon {
	 background-color: var(--gray-200);
	 display: inline-flex;
	 align-items: center;
	 justify-content: center;
	 width: 120px;
	 height: 120px;
	 border-radius: 5px;
	 position: relative;
	 margin-bottom: 18px;
}
 @media (max-width: 767px) {
	 .flow-icon {
		 width: 50px;
		 height: 50px;
		 margin-bottom: 10px;
	}
	 .flow-icon svg {
		 width: 30px;
		 height: auto;
	}
}
 .status {
	 width: 36px;
	 height: 36px;
	 border-radius: 50%;
	 display: flex;
	 align-items: center;
	 justify-content: center;
	 position: absolute;
	 bottom: -18px;
	 left: 50%;
	 margin-left: -18px;
}
 @media (max-width: 767px) {
	 .status {
		 width: 20px;
		 height: 20px;
		 margin-left: -10px;
		 bottom: -10px;
	}
	 .status svg {
		 width: 10px;
		 height: auto;
	}
}
 .status-success {
	 background-color: var(--primary);
}
 .status-error {
	 background-color: var(--danger);
	 display: none;
}
 .fail .status-success {
	 display: none;
}
 .fail .status-error {
	 display: flex;
}
 .security-check {
	 margin: 60px 0;
}
 @media (max-width: 767px) {
	 .security-check {
		 margin: 40px 0;
	}
}
</style>
</head>
